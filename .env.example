# CFR Analyzer Environment Variables
# Copy this file to .env.local and update the values

export ECFR_ADMIN_TOKEN="your-admin-token-here"
export ECFR_DB_USER="ecfr-app"
export ECFR_DB_PASS=""
export ECFR_DB_HOST="localhost"
export ECFR_DB_PORT="5432"
export ECFR_DB_NAME="ecfr"
export ECFR_DB_INSTANCE_CONNECTION_NAME=""
export ECFR_DEVELOPMENT="true"

# To use:
# 1. Copy this file: cp .env.example .env.local
# 2. Update ECFR_ADMIN_TOKEN with a unique value
# 3. Load the variables: source .env.local
